<p id="notice"><%= notice %></p>

<h1>Listing Lintitems</h1>

<table>
  <thead>
    <tr>
      <th>Product</th>
      <th>Order</th>
      <th>Quantity</th>
      <th>Cart</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @lintitems.each do |lintitem| %>
      <tr>
        <td><%= lintitem.product_id %></td>
        <td><%= lintitem.order_id %></td>
        <td><%= lintitem.quantity %></td>
        <td><%= lintitem.cart_id %></td>
        <td><%= link_to 'Show', lintitem %></td>
        <td><%= link_to 'Edit', edit_lintitem_path(lintitem) %></td>
        <td><%= link_to 'Destroy', lintitem, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Lintitem', new_lintitem_path %>
